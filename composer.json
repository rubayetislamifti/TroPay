{
    "name": "trodevit/tropay",
    "description": "Testing",
    "type": "library",
    "require": {
        "illuminate/support": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TrodevIT\\TroPay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TrodevIT\\TroPay\\TroPayServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "rubayet",
            "email": "rubayetislam16@gmail.com"
        }
    ],
    "minimum-stability": "dev",

    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --provider=\"TrodevIT\\TroPay\\TroPayServiceProvider\" --tag=config --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --provider=\"TrodevIT\\TroPay\\TroPayServiceProvider\" --tag=config --force"
        ]
    }

}
